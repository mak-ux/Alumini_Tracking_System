<%- include('./partials/header.ejs')%>
   <!-- Navbar content -->
     
   
  
  
  <!-- crosel content -->

  
	<section>
		<div class="gap no-gap">
			<div class="event-detail">
				<div class="blog-image"><img src="/uploads/<%= event.img[0] %>" alt=""></div>
				<div class="event-meta">
					<div class="event-timing">
                        <h5>welcome to <%= event.name %></h5><br><br>
						<a href="#" title="" class="postdate"><%=event.date %> <span><%= event.month %></span> <%= event.year %></a>
						<h5><i class="lni lni-timer"></i> From: <span><%= event.time %>: <%= event.clock %></span></h5>
						
					</div>
					<div class="g-mapfull" style="height: 250px; float: left; width: 100%;">
						<div id="map-canvas"></div>
					</div>
					<h2><%= event.name %></h2>
					
					<p>
				        <%= event.description %>
					</p>
					<h5 class="main-title"></h5>
					<div class="row merged10">
						
                        <% if( event){ %>

							<div class="col-6">
                               
								<img src= "/uploads/<%= event.img %>" alt="">
							</div>
							
                            <% } %>

					</div>
					<p>
						<%= event.description %>
					</p>
					
					<a class="prv" href="<%= event.websiteURL %>" title=""> Join Now <i class="lni lni-arrow-right"></i></a>
				</div>
			</div>
		</div>
	</section>
  
  <!-- crosel content -->
 
